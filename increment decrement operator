#include<iostream>
using namespace std;
int main(){
    int a,b;
    cin>>a>>b;
    /*cout<<"a&b = "<<(a&b)<<endl;
    cout<<"a|b = "<<(a|b)<<endl;
    cout<<"a^b = "<<(a^b)<<endl;
    cout<<"~a = "<<(~a)<<endl;
    cout<<"~b = "<<(~b)<<endl;
    cout<<(a<<1)<<endl;
    cout<<(a>>2)<<endl;
    cout<<(b>>1)<<endl;
    cout<<(b<<2)<<endl;
    */
    int i=5;
    cout<<i++<<endl;
    //5 i=6
    cout<<++i<<endl;
    //7 i=7
    cout<<i--<<endl;
    //7 i=6
    cout<<--i<<endl;
    //5 i=5
    return 0;
}
