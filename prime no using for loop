#include<iostream>
using namespace std;
int main(){
    int n;
    cin>>n;
    bool isprime =1;
    for(int i=2;i<n;i++){
        if(n%i==0){
            isprime=0;
            break;
        }
    }
    if(isprime==1){
        cout<<n<<" is a  prime no"<<endl;
    }
    else{
        cout<<n<<" is not a prime no"<<endl;
    }
    
    return 0;     
}
